<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/custom_dialog_date_icon_background">

    <LinearLayout
        android:gravity="center_horizontal"
        android:layout_marginTop="@dimen/ten_sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <ImageView
            android:scaleType="fitCenter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/ten_sp"
            android:src="@drawable/resolution"/>
        <TextView
                  android:id="@+id/textweekmitem"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginLeft="@dimen/ten_sp"
                  android:gravity="left|center_vertical"
                  android:text="dates"
                  android:textColor="@color/white_color"
                  android:textSize="@dimen/twentee_sp"
                  android:textStyle="bold"/>
    </LinearLayout>


    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/twentee_sp"
        android:layout_marginLeft="@dimen/twentee_sp"
        android:layout_marginRight="@dimen/twentee_sp"
        android:id="@+id/listviewprojectdate"
        android:divider="@drawable/listview_divider">

    </ListView>
</LinearLayout>