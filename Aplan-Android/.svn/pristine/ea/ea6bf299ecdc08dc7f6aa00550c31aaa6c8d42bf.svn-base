                        android:paddingLeft="@dimen/ten_sp"
                        android:textSize="@dimen/twentee_sp"/>
                    <EditText
                        android:id="@+id/edittext3Number"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/twentee_sp"
                        android:layout_marginRight="@dimen/twentee_sp"
                        android:background="@drawable/rounded_rectangle_white"
                        android:paddingLeft="@dimen/ten_sp"
                        android:textSize="@dimen/twentee_sp"/>
                    </FrameLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/fiftee_sp"
                android:layout_marginLeft="@dimen/twentee_sp"
                android:layout_marginRight="@dimen/twentee_sp"
                android:layout_marginTop="@dimen/ten_sp"
                android:gravity="center_vertical">

                <TextView
                    android:id="@+id/textview4"
                    android:layout_width="@dimen/hundred_sp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/key_text4"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/twentee_sp"/>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <EditText
                        android:id="@+id/edittext4"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/twentee_sp"
                        android:layout_marginRight="@dimen/twentee_sp"
                        android:background="@drawable/rounded_rectangle_white"
                        android:paddingLeft="@dimen/ten_sp"
                        android:textSize="@dimen/twentee_sp"/>
                    <EditText
                        android:id="@+id/edittext4Number"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/twentee_sp"
                        android:layout_marginRight="@dimen/twentee_sp"
                        android:background="@drawable/rounded_rectangle_white"
                        android:paddingLeft="@dimen/ten_sp"
                        android:textSize="@dimen/twentee_sp"/>
                    </FrameLayout>

