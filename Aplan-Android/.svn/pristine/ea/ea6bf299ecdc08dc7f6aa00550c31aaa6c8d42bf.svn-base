                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <EditText
                        android:id="@+id/edittext8"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/twentee_sp"
                        android:layout_marginRight="@dimen/twentee_sp"
                        android:background="@drawable/rounded_rectangle_white"
                        android:paddingLeft="@dimen/ten_sp"
                        android:textSize="@dimen/twentee_sp"/>
                    <EditText
                        android:id="@+id/edittext8Number"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/twentee_sp"
                        android:layout_marginRight="@dimen/twentee_sp"
                        android:background="@drawable/rounded_rectangle_white"
                        android:paddingLeft="@dimen/ten_sp"
                        android:textSize="@dimen/twentee_sp"/>
                    </FrameLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/fiftee_sp"
                android:layout_marginLeft="@dimen/twentee_sp"
                android:layout_marginRight="@dimen/twentee_sp"
                android:layout_marginTop="@dimen/ten_sp"
                android:gravity="center_vertical">

                <TextView
                    android:id="@+id/textview9"
                    android:layout_width="@dimen/hundred_sp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/key_text9"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/twentee_sp"/>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <EditText
                        android:id="@+id/edittext9"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/twentee_sp"
                        android:layout_marginRight="@dimen/twentee_sp"
                        android:background="@drawable/rounded_rectangle_white"
                        android:paddingLeft="@dimen/ten_sp"
                        android:textSize="@dimen/twentee_sp"/>
                    <EditText
                        android:id="@+id/edittext9Number"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/twentee_sp"
                        android:layout_marginRight="@dimen/twentee_sp"
                        android:background="@drawable/rounded_rectangle_white"
                        android:paddingLeft="@dimen/ten_sp"
                        android:textSize="@dimen/twentee_sp"/>
                    </FrameLayout>



            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/fiftee_sp"
                android:layout_marginLeft="@dimen/twentee_sp"
                android:layout_marginRight="@dimen/twentee_sp"
                android:layout_marginTop="@dimen/ten_sp"
                android:gravity="center_vertical">

                <TextView
                    android:id="@+id/textview10"
                    android:layout_width="@dimen/hundred_sp"
                    android:layout_height="wrap_content"
                    android:gravity="right"
                    android:text="@string/key_text10"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/twentee_sp"/>

                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <EditText
                        android:id="@+id/edittext10"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="@dimen/twentee_sp"
                        android:layout_marginRight="@dimen/twentee_sp"
