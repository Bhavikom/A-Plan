            <RelativeLayout

                android:id="@+id/reletiveswitch"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/twentee_sp"
                android:layout_marginTop="@dimen/ten_sp"
                android:layout_marginBottom="@dimen/fourtee_sp"
                android:gravity="center_vertical">

                <TextView
                    android:layout_centerVertical="true"
                    android:id="@+id/txtText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@+id/switchnumber"
                    android:layout_marginRight="@dimen/twentee_sp"
                    android:text="Text"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/twentee_sp"/>

                <TextView
                    android:layout_centerVertical="true"
                    android:id="@+id/textnumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/twentee_sp"
                    android:text="Number"
                    android:textColor="@color/black_color"
                    android:textSize="@dimen/twentee_sp"/>

                <Switch
                    android:layout_width="@dimen/seventee_sp"
                    android:layout_height="@dimen/twentee_five_sp"
                    android:id="@+id/switchnumber"
                    android:track="@drawable/switch_track_selector"
                    android:switchMinWidth="50dp"
                    android:layout_marginRight="@dimen/twentee_sp"
                    android:thumb="@drawable/switch_thumb_selector"
                    android:layout_alignBaseline="@+id/textnumber"
                    android:layout_alignBottom="@+id/textnumber"
                    android:layout_toLeftOf="@+id/textnumber"
                    android:layout_toStartOf="@+id/textnumber"
                    />
