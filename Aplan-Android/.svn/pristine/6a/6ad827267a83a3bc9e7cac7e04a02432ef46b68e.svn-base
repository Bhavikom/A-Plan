<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@drawable/custom_dialog_date_icon_background"
              android:orientation="vertical">

    <TextView
        android:id="@+id/text_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/twentee_sp"
        android:layout_marginTop="@dimen/twentee_sp"
        android:gravity="left|center_vertical"
        android:text="Profiles"
        android:textColor="@color/white_color"
        android:textSize="@dimen/twentee_sp"
        android:textStyle="bold"/>

    <ListView
        android:id="@+id/listview_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/twentee_sp"
        android:layout_marginRight="@dimen/twentee_sp"
        android:layout_marginTop="@dimen/twentee_sp"
        android:divider="@drawable/listview_divider">
    </ListView>

    <View
        android:layout_width="@dimen/two_hundred"
        android:layout_height="1sp"
        android:layout_marginLeft="@dimen/twentee_sp"
        android:layout_marginRight="@dimen/twentee_sp"
        android:background="@drawable/listview_divider"
        />

    <LinearLayout
        android:id="@+id/linear_add_profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/twentee_sp"
        android:layout_marginRight="@dimen/twentee_sp"
        android:layout_marginBottom="@dimen/twentee_sp"
        android:layout_marginTop="@dimen/twentee_sp"
        android:gravity="center_vertical">

        <ImageView
            android:id="@+id/imageview_add"
            android:layout_width="@dimen/fiftee_sp"
            android:layout_height="@dimen/fiftee_sp"
            android:scaleType="fitXY"
            android:src="@drawable/add_profile"/>

        <TextView
            android:id="@+id/text_addprofile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical"
            android:padding="@dimen/ten_sp"
            android:text="Add Profile"
            android:textColor="@color/white_color"
            android:textSize="@dimen/twentee_sp"
            android:textStyle="bold"/>
    </LinearLayout>
</LinearLayout>