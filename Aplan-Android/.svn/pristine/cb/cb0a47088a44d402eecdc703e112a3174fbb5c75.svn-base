<RelativeLayout android:id="@+id/reletive1"
                xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                tools:context="com.ssoft.aplanproject.fragment.FragmentTable">

    <LinearLayout
        android:id="@+id/lineartop"
        android:layout_marginLeft="@dimen/twentee_sp"
        android:layout_marginRight="@dimen/twentee_sp"
        android:layout_marginTop="@dimen/fiftee_sp"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:weightSum="3">
        <TextView
            android:textColor="@color/table_textcolor"
            android:gravity="left"
            android:layout_weight="1.5"
            android:textSize="@dimen/twentee_five_sp"
            android:layout_width="0sp"
            android:layout_height="wrap_content"
            android:text="@string/name"/>
        <TextView
            android:textColor="@color/table_textcolor"
            android:gravity="left"
            android:layout_weight="0.75"
            android:textSize="@dimen/twentee_five_sp"
            android:layout_width="0sp"
            android:layout_height="wrap_content"
            android:text="@string/show"/>
        <TextView
            android:textColor="@color/table_textcolor"
            android:gravity="left"
            android:layout_weight="0.75"
            android:textSize="@dimen/twentee_five_sp"
            android:layout_width="0sp"
            android:layout_height="wrap_content"
            android:text="@string/width"/>
    </LinearLayout>
    <!--<ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/listviewTable"
        android:layout_margin="@dimen/twentee_sp">

    </ListView>-->
    <view
        class="android.support.v7.widget.RecyclerView"
        android:layout_marginTop="@dimen/twentee_sp"
        android:layout_below="@+id/lineartop"
        android:id="@+id/listviewTable"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="@dimen/twentee_sp"
        android:layout_marginRight="@dimen/twentee_sp">
    </view>

</RelativeLayout>
