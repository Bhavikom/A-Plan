<LinearLayout
    android:id="@+id/linear_suppport_main"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white_color"
    android:orientation="vertical"
    >

    <RelativeLayout
        android:id="@+id/relative_aboutus"
        android:layout_width="match_parent"
        android:layout_height="@dimen/eightee_sp"
        android:layout_marginTop="@dimen/hundred_sp"
        android:background="@color/select_filter"
        >

        <ImageView
            android:id="@+id/imageview_aboutus"
            android:layout_width="@dimen/fiftee_sp"
            android:layout_height="@dimen/fiftee_sp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/twentee_sp"
            android:src="@drawable/support_about_us"/>

        <TextView
            android:id="@+id/textview_aboutus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/twentee_sp"
            android:layout_toRightOf="@+id/imageview_aboutus"
            android:text="@string/aboutus"

            android:textColor="@color/black"
            android:textSize="@dimen/twentee_sp"/>

        <LinearLayout
            android:id="@+id/linear1"
            android:layout_marginRight="@dimen/ten_sp"
            android:gravity="center"
            android:layout_alignParentRight="true"
            android:background="#ffffff"
            android:layout_width="wrap_content"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/imgarrow2"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"

                android:src="@drawable/next"/>
        </LinearLayout>

        <TextView
            android:layout_toLeftOf="@+id/linear1"
            android:id="@+id/textview_version_no"
            android:text="version"
            android:layout_marginRight="@dimen/ten_sp"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/textview_color_grey"
            android:textSize="@dimen/twentee_sp"/>

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linearMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/fiftee_sp"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/relative_help"
            android:layout_width="match_parent"
            android:layout_height="@dimen/eightee_sp"
            android:background="@color/select_filter">

            <TextView
                android:id="@+id/textview_help"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/twentee_sp"
                android:layout_toRightOf="@+id/imageview_help"
                android:text="@string/help"
                android:textColor="@color/black"
                android:textSize="@dimen/twentee_sp" />

            <LinearLayout
                android:id="@+id/linear2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/ten_sp"
                android:background="#ffffff"
                android:gravity="center">

                <ImageView
                    android:id="@+id/imgarrow"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/next" />
            </LinearLayout>

            <TextView
                android:id="@+id/textview_send_feedback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/ten_sp"
                android:layout_toLeftOf="@+id/linear2"
                android:text="version"
                android:textColor="@color/textview_color_grey"
                android:textSize="@dimen/twentee_sp" />

            <ImageView
                android:layout_marginLeft="@dimen/twentee_sp"
                android:id="@+id/imageview_help"
                android:layout_width="@dimen/fiftee_sp"
                android:layout_height="@dimen/fiftee_sp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:src="@drawable/support_help" />
        </RelativeLayout>

        <RelativeLayout
            android:visibility="visible"
            android:id="@+id/relative_contact_adamin"
            android:layout_width="match_parent"
            android:layout_height="@dimen/eightee_sp"
            android:layout_marginTop="@dimen/fiftee_sp"
            android:background="@color/select_filter">
            <TextView
                android:id="@+id/textview_message"
                android:gravity="center"
                android:text="@string/contact_adminstrator"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/twentee_sp"
                android:layout_marginRight="@dimen/twentee_sp"
                android:textSize="@dimen/twentee_sp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/black_color" />
        </RelativeLayout>

        <View
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="2sp"
            android:background="#cccccc"/>

        <RelativeLayout
            android:visibility="gone"
            android:id="@+id/relative_faqs"
            android:layout_width="match_parent"
            android:layout_height="@dimen/eightee_sp"
            >
            <ImageView
                android:id="@+id/imageview_faq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/twentee_sp"
                android:src="@drawable/support_faq"/>
            <TextView
                android:layout_marginLeft="@dimen/twentee_sp"
                android:layout_toRightOf="@+id/imageview_faq"
                android:layout_centerVertical="true"
                android:id="@+id/textview_faq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/faqs"
                android:textColor="@android:color/black"
                android:textSize="@dimen/twentee_sp"/>
            <ImageView
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:src="@drawable/next"/>
        </RelativeLayout>

    </LinearLayout>


    <RelativeLayout
        android:visibility="gone"
        android:id="@+id/relative_contact"
        android:layout_width="match_parent"
        android:layout_height="@dimen/eightee_sp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="@dimen/twentee_sp"
        android:layout_marginRight="@dimen/twentee_sp"
        android:layout_marginTop="@dimen/fiftee_sp"
        android:background="@drawable/rounded_rectangle_white"
        >

        <ImageView
            android:id="@+id/imageview_contact"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/twentee_sp"
            android:src="@drawable/support_contact"/>

        <TextView
            android:layout_marginLeft="@dimen/twentee_sp"
            android:layout_toRightOf="@+id/imageview_contact"
            android:layout_centerVertical="true"
            android:id="@+id/textview_contact"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/contect"
            android:textColor="@android:color/black"
            android:textSize="@dimen/twentee_sp"/>

        <ImageView
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:src="@drawable/next"/>


    </RelativeLayout>

</LinearLayout>