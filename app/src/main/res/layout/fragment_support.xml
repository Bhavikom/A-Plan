<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/linear_suppport_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/grey_light"
    android:orientation="vertical">


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="@dimen/hundred_sp"
        android:background="#A1A1A1" />


    <RelativeLayout
        android:id="@+id/relative_aboutus"
        android:layout_width="match_parent"
        android:layout_height="@dimen/fiftee_sp"
        android:layout_marginLeft="@dimen/ten_sp"
        android:layout_marginRight="@dimen/ten_sp"
        android:background="@drawable/rounded_rectangle_white1">


        <ImageView
            android:id="@+id/imageview_aboutus"
            android:layout_width="@dimen/fourtee_sp"
            android:layout_height="@dimen/fourtee_sp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/five_sp"
            android:src="@drawable/support_about_us" />

        <TextView
            android:id="@+id/textview_aboutus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/ten_sp"
            android:layout_toRightOf="@+id/imageview_aboutus"
            android:text="@string/aboutus"

            android:textColor="@color/black"
            android:textSize="@dimen/fifteen_sp" />

        <ImageView
            android:id="@+id/imgarrow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/next" />

        <TextView
            android:gravity="right"
            android:id="@+id/textview_version_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textview_aboutus"
            android:layout_alignBottom="@+id/textview_aboutus"
            android:layout_marginEnd="8dp"
            android:layout_marginRight="@dimen/five_sp"
            android:layout_toLeftOf="@+id/imgarrow2"
            android:layout_toStartOf="@+id/imgarrow2"
            android:text="version"
            android:textColor="@color/textview_color_grey"
            android:textSize="@dimen/thirteen_sp" />

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/linearMain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/ten_sp"
        android:layout_marginRight="@dimen/ten_sp"
        android:layout_marginTop="@dimen/fiftee_sp"
        android:background="@drawable/rounded_rectangle_white"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/relative_help"
            android:layout_width="match_parent"
            android:layout_height="@dimen/fiftee_sp"
            android:background="@drawable/rounded_rectangle_white1">

            <ImageView
                android:id="@+id/imageview_help"
                android:layout_width="@dimen/fourtee_sp"
                android:layout_height="@dimen/fourtee_sp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/five_sp"
                android:src="@drawable/support_help" />

            <TextView
                android:id="@+id/textview_help"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/ten_sp"
                android:layout_toRightOf="@+id/imageview_help"
                android:text="@string/help"
                android:textColor="@color/black"
                android:textSize="@dimen/fifteen_sp" />

            <ImageView
                android:id="@+id/imgarrow"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/next" />

            <TextView
                android:gravity="right"
                android:id="@+id/textview_send_feedback"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/five_sp"
                android:layout_marginRight="@dimen/five_sp"
                android:layout_toLeftOf="@+id/imgarrow"
                android:layout_toRightOf="@+id/textview_help"
                android:text="version"
                android:textColor="@color/textview_color_grey"
                android:textSize="@dimen/thirteen_sp" />
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/relative_faqs"
            android:layout_width="match_parent"
            android:layout_height="@dimen/eightee_sp"
            android:visibility="gone">

            <ImageView
                android:id="@+id/imageview_faq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/twentee_sp"
                android:src="@drawable/support_faq" />

            <TextView
                android:id="@+id/textview_faq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/twentee_sp"
                android:layout_toRightOf="@+id/imageview_faq"
                android:text="@string/faqs"
                android:textColor="@android:color/black"
                android:textSize="@dimen/twentee_sp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/next" />
        </RelativeLayout>


    </LinearLayout>


    <RelativeLayout
        android:id="@+id/relative_contact_adamin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/fiftee_sp"
        android:layout_marginTop="@dimen/fiftee_sp"
        android:background="@color/white_color"
        android:visibility="visible">

        <TextView
            android:id="@+id/textview_message"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/twentee_sp"
            android:layout_marginRight="@dimen/twentee_sp"
            android:gravity="left"
            android:text="@string/contact_adminstrator"
            android:textColor="@color/black_color"
            android:textSize="@dimen/twelve_sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/relative_contact"
        android:layout_width="match_parent"
        android:layout_height="@dimen/eightee_sp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="@dimen/twentee_sp"
        android:layout_marginRight="@dimen/twentee_sp"
        android:layout_marginTop="@dimen/fiftee_sp"
        android:background="@drawable/rounded_rectangle_white"
        android:visibility="gone">

        <ImageView
            android:id="@+id/imageview_contact"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/twentee_sp"
            android:src="@drawable/support_contact" />

        <TextView
            android:id="@+id/textview_contact"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/twentee_sp"
            android:layout_toRightOf="@+id/imageview_contact"
            android:text="@string/contect"
            android:textColor="@android:color/black"
            android:textSize="@dimen/twentee_sp" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@drawable/next" />


    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#A1A1A1" />


</LinearLayout>